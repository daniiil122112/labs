#include <stdio.h>
#define MAXLINE 1000
int prv(char mass[], int begin, int end);
main()
{
   int wlen;
   char str[MAXLINE];
    int l,F,id,i,pos,k;
   printf("write a string:\n");
   gets(str);
   l=0;
   for (i=0;i<MAXLINE;++i)
    {
    if(str[i]!='\0')
       ++l;
     else
         break;
      }
     F=0;
    for(i = 0; i <= l; i++)
    {
     if(str[i]==' '|| str[i]=='.' || str[i]==',' || str[i]=='\t' || i==l)
      {
         if (F == 1)
         {
           wlen=i-pos;
          if(prv(str,pos,i)==0 && (str[pos]=='a' || str[pos]=='i' || str[pos]=='u' || str[pos]=='y' || str[pos]=='e' || str[pos]=='o'(str[pos]=='A' || str[pos]=='I' || str[pos]=='U' || str[pos]=='Y' || str[pos]=='E' || str[pos]=='O' ))
         {
          for (k = pos; k <= l; k++)
               str[k]=str[k+wlen];
          l=l-wlen;	
           i=i-wlen;
           }
          }
          F = 0;
         }
          else if (F==0 )
          {
              F = 1;
              pos=i;
          }
         }
        puts(str);
      return 0;
}
int prv(char str[], int begin, int end)
{
    int F,i,f1;
     F=f1=0;
    for (i=begin; i < end; i++)
     {
     if ((str[i]<'A' && str[i]>'Z') || (str[i]<'a' && str[i]>'z'))
        F=1;
       }
   return F;
}
